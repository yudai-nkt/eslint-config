{
  "name": "@yudai-nkt/eslint-config",
  "version": "0.1.0",
  "description": "My shareable config for ESLint",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./*": "./presets/*.js"
  },
  "files": [
    "index.js",
    "presets"
  ],
  "engines": {
    "node": ">=16.9.0"
  },
  "scripts": {
    "format": "prettier --write *.js presets __tests__",
    "lint": "eslint --fix *.js presets __tests__/*.mjs",
    "test": "uvu __tests__ --ignore utils.mjs --ignore dummy.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yudai-nkt/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Yudai Nakata <yudai.nkt@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yudai-nkt/eslint-config/issues"
  },
  "homepage": "https://github.com/yudai-nkt/eslint-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/eslint": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "uvu": "^0.5.2"
  }
}
